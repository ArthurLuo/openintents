<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="oi_distribution_app_name">OI Distribution Library</string>
    
	<!-- ***************************
	     Distribution
	     *************************** -->
	
	<!-- EULA -->	
	<!-- EULA (End user license agreement). %s is an application name, e.g. "OI Notepad EULA". 
		Translation should be short, since this is a title. -->
	<string name="oi_distribution_eula_title">%s EULA</string>
	
	<!-- EULA welcome message. %s is an application name, e.g. "... (EULA) for OI Notepad".
		Translation can be as long as required. -->
	<string name="oi_distribution_eula_message">Please read and accept the following End User License Agreement (EULA) for %s.</string>
	
	<!-- Button: Accept terms of EULA. -->
	<string name="oi_distribution_eula_accept">Accept</string>
	
	<!-- Button: Refuse terms of EULA. -->
	<string name="oi_distribution_eula_refuse">Cancel</string>
	
	<!-- New version -->
	<!-- Title for a new version. %1$s is the application name (e.g. "OI Notepad") and %2$s is the version number (e.g. "1.2.0"). -->
	<string name="oi_distribution_newversion_title">%1$s %2$s</string>
	
	<!-- Welcome message. %s is an application name. -->
	<string name="oi_distribution_newversion_message">Welcome to this new version of %s.</string>
	
	<string name="oi_distribution_newversion_recent_changes">Recent changes:</string>
	
	<!-- Button: Continue to main application. -->
	<string name="oi_distribution_newversion_continue">Continue</string>
	
	<!-- Update -->
	<!-- Menu command to look for updates. -->
    <string name="oi_distribution_menu_update">Check updates</string>
	<string name="oi_distribution_update_box_text">Please check manually for updates or install OI Update
for automatic notifications.
Current version: %s
	</string>
	<string name="oi_distribution_update_check_now">Check now.</string>
	<string name="oi_distribution_update_get_updater">Get OI Updater.</string>
	<string name="oi_distribution_update_error">Download application not available.</string>
	
    <!-- About -->
    <!-- Menu command to open the "about" or "info" dialog. -->
    <string name="oi_distribution_about">About</string>
	<string name="oi_distribution_aboutapp_not_available">Version %s\n\nNo compatible extension available to display full info (contact, credits, license).</string>
    <string name="oi_distribution_aboutapp_get">Get OI About</string>
    
    <!-- File Manager -->
    <!-- Dialog message. -->
    <string name="oi_distribution_filemanager_not_available">No compatible file manager is available.</string>
    <string name="oi_distribution_filemanager_get_oi_filemanager">Get OI File Manager</string>
    
    <!-- Safe -->
    <!-- Dialog message. -->
    <string name="oi_distribution_safe_not_available">No compatible application for encryption is available.</string>
    <string name="oi_distribution_safe_not_available_decrypt">No compatible application for decryption is available.\n\nNote: You have to restore the master key from a backup file in order to access previously encrypted data.</string>
    <string name="oi_distribution_safe_get_oi_filemanager">Get OI Safe</string>
    
</resources>
