<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.openintents.samples">
    
    <!-- required in order to getCurrentLocation -->
	<uses-permission android:name="android.permission.ACCESS_GPS" />
	<uses-permission android:name="android.permission.ACCESS_LOCATION" />
    
    <application android:icon="@drawable/locations_favorite_application001a">
        <activity android:name=".FavoriteLocations" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            	<category android:name="org.openintents.category.MAIN" />
			</intent-filter>
        </activity>
    </application>
</manifest>
