<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.openintents.samples.presentpicker">
    
    <uses-permission id="android.permission.READ_CONTACTS" />
    
    <application android:icon="@drawable/icon_openintents_sample">
        <activity class=".PresentPicker" android:label="@string/app_name">
            <intent-filter>
                <action android:value="android.intent.action.MAIN" />
                <category android:value="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest> 