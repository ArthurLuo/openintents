<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.openintents.applications.newsreader">
    <application android:icon="@drawable/news_application001a">

        <activity android:name=".Newsreader" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="org.openintents.category.MAIN" />
            </intent-filter>
        </activity>

       <activity android:name=".AFeedSettings" android:label="@string/app_name">
                    <!-- This filter says that we can create a new note inside
                 of a directory of notes. -->
            <intent-filter>
                <action android:name="android.intent.action.INSERT" />
                <!--  
                <category android:name="android.intent.category.DEFAULT" />
                <type android:name="vnd.openintents.cursor.dir/vnd.openintents.rssfeed" />
                -->
            </intent-filter>
        </activity>        
        <activity android:name=".ServiceSettings" android:label="@string/app_name">
        </activity> 
		<activity android:name=".AFeedMessages" android:label="@string/app_name">
        </activity> 
		<activity android:name=".FeedMessages" android:label="@string/app_name">
        </activity> 
				<!-- news services -->
		<service android:name=".fdNewsReaderService"
				 android:enabled="true"
		>

	</service>

    </application>
</manifest> 